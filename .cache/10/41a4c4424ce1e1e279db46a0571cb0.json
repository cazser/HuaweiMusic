  .effect-1{\r\n        background: url('../svg/effect-no-move.svg') 0 0 no-repeat;\r\n        width: 70vw;\r\n        height: 70vw;\r\n        animation: rotate 20s linear  infinite;\r\n    }\r\n    .effect-2{\r\n        background: url('../svg/effect-move1.svg') 0 0 no-repeat;\r\n        width: 60vw;\r\n        height: 60vw;\r\n        animation: rotate 10s linear  infinite reverse;\r\n    }\r\n\r\n    \r\n    .effect-3{\r\n        background: url('../svg/effect-move2.svg') 0 0 no-repeat;\r\n        width: 24vw;\r\n        height: 24vw;\r\n        animation: rotate 10s linear  infinite;\r\n    }\r\n\r\n    \r\n    }\r\n\r\n    .lyric{\r\n        p{\r\n            font-size: 16px;\r\n            color: $color1;\r\n            margin-top: 8px;\r\n            &.current{\r\n                color:$color;\r\n            }\r\n        }\r\n    }\r\n}\r\n.footer{\r\n    border: 1px solid white;\r\n    .buttons{\r\n        display: flex;\r\n        justify-content: center;\r\n        margin-top: 20px;\r\n        svg{\r\n            width: 24px;\r\n            height: 24px;\r\n            margin: 0 20px;\r\n        }\r\n    }\r\n}\r\n\r\n@keyframes rotate {\r\n    0%{\r\n        transform: rotate(0deg);\r\n    }\r\n\r\n    100%{\r\n        transform: rotate(360deg);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"7c371e4a33ae19e4a29fad20d4e3a62d","cacheData":{"env":{}}}